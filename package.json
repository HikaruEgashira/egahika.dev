    "next": "11.0.1",
    "next-seo": "^4.28.1",
    "next-transpile-modules": "8.0.0",
    "notion-client": "4.8.2",
    "notion-types": "4.8.2",
    "notion-utils": "4.8.6",
    "p-map": "4.0.0",
    "p-memoize": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-notion-x": "4.8.3",
    "react-props-x": "0.0.2",
    "react-tweet-embed": "1.2.2",
    "react-use": "17.2.4",
